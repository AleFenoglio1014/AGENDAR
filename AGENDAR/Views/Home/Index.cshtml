<!-- SECTION CONTACTO -->

<section id="Contacto">
    <div class="titulo ">
        <h1 class="colorTitulo" style="font-weight:bold"> CONTACTO </h1>
        <hr class="sumbrayado" />
    </div>
    <div class="contacto">
        <form>
            <div class="row centrado">
                <div class="col-md-6">
                    <div class="inputContacto">
                        <input type="text" class="form-control " id="validarNombre" name="validarNombre" required placeholder="Nombre:">
                    </div>
                    <div class="inputContacto">
                        <input type="email" class="form-control " id="validarEmail" name="validarEmail" required placeholder="Email:">
                    </div>
                    <div class="inputContacto">
                        <input type="text" class="form-control " id="validarAsunto" name="validarAsunto" required placeholder="Asunto:">
                    </div>
                </div>
                <div class="col-md-5 .ml-auto">
                    <div class="inputContacto">

                        <textarea class="form-control" id="validationMensaje" name="validationMensaje" required placeholder="Mensaje:" cols="40" rows="5" min="50"></textarea>
                    </div>
                </div>
            </div>
            <div class="  flex-grow-1 centradobotones">
                <button class="btn btn-success tamañobotones " type="submit" name="submit">Enviar</button>
                @*<button class="btn btn-danger tamañobotones " type="reset" name="reset">Borrar</button>*@
            </div>
        </form>
    </div>
</section>

